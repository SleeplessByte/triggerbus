{
  "name": "triggerbus",
  "version": "3.0.0",
  "description": "Simple eventbus you can trigger",
  "main": "dist/triggerbus.js",
  "umd:main": "dist/triggerbus.umd.js",
  "module": "dist/triggerbus.mjs",
  "source": "src/triggerbus.ts",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "microbundle",
    "predev": "yarn clean",
    "dev": "yarn watch",
    "watch": "microbundle watch",
    "prepublish": "yarn build",
    "clean": "rimraf dist && rimraf .rp2t2_cache",
    "test": "karma start --single-run",
    "test:ci": "karma start --single-run --browsers Firefox",
    "test:watch": "karma start --no-single-run --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SleeplessByte/triggerbus.git"
  },
  "keywords": [
    "event",
    "javascript",
    "eventbus"
  ],
  "author": "Derk-Jan Karrenbeld",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SleeplessByte/triggerbus/issues"
  },
  "homepage": "https://github.com/SleeplessByte/triggerbus#readme",
  "devDependencies": {
    "@types/jasmine": "^3.3.5",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-typescript": "^3.0.13",
    "microbundle": "^0.9.0",
    "rimraf": "^2.6.2",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "karma-typescript-es6-transform": "^1.0.4"
  }
}
