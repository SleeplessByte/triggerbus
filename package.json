{
  "name": "triggerbus",
  "version": "1.2.0",
  "description": "Simple eventbus you can trigger",
  "main": "dist/triggerbus.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=production rollup -c && cpx src/**/*.d.ts dist",
    "watch": "rollup -wc",
    "test": "karma start --single-run",
    "test:ci": "karma start --single-run --browsers Firefox",
    "test:watch": "karma start --no-single-run --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SleeplessByte/triggerbus.git"
  },
  "keywords": [
    "event",
    "javascript",
    "eventbus"
  ],
  "author": "Derk-Jan Karrenbeld",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SleeplessByte/triggerbus/issues"
  },
  "homepage": "https://github.com/SleeplessByte/triggerbus#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-preset-minify": "^0.5.0-alpha.a24dd066",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-rollup-preprocessor": "^6.0.0",
    "rollup": "^0.60.7",
    "rollup-plugin-babel": "^4.0.0-beta.5"
  },
  "dependencies": {}
}
